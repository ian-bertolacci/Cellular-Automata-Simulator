$states = 2
$dimension = 2
$radius = 1
;

int count = 0;
for( int i = 0; i < 9; i = i + 1){
	if( neighborhood[i] == 1 ) count = count + 1;
}


if( neighborhood[4] == 1 ) count = count - 1;


if( count == 3 | count == 7 | count == 8 ) return 1;
else if( count == 4 ) return 0;
else return neighborhood[4];